{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": ["error", "unix"],
        "semi": ["error", "always"],
        "no-tabs": "error",
        "indent": ["error", 2],
        "consistent-return": "error",
        "dot-notation": "error"
      }
}
